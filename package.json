{
  "name": "CanonChain-Wallet",
  "productName": "CanonChain Wallet",
  "version": "0.9.7",
  "keywords": [
    "block-wallet"
  ],
  "author": "CanonChain",
  "description": "CanonChain Wallet",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "start": "./node_modules/.bin/electron-rebuild && node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "rebuild": "./node_modules/.bin/electron-rebuild"
  },
  "build": {
    "remoteBuild": true,
    "publish": [
      {
        "provider": "generic",
        "url": "https://canonchain-public.oss-cn-hangzhou.aliyuncs.com/wallet/latest/"
      }
    ],
    "productName": "CanonChain Wallet",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "!build/win-unpacked/d3dcompiler_47.dll",
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/icons/icon.icns"
    },
    "win": {
      "icon": "static/icons/icon.ico"
    },
    "linux": {
      "icon": "static/icons"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "czr": "git+https://github.com/canonchain/czr.js.git",
    "download": "^7.0.0",
    "electron-updater": "^4.0.6",
    "element-ui": "^2.8.2",
    "fs-extra": "^5.0.0",
    "lodash-id": "^0.14.0",
    "log4js": "3.0.0",
    "lowdb": "^1.0.0",
    "qrcode": "^1.2.0",
    "underscore": "^1.9.0",
    "vue": "^2.5.22",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^7.6.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-electron": "^1.0.3"
  },
  "devDependencies": {
    "ajv": "^6.7.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.4.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.3",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-builder": "^20.38.5",
    "mini-css-extract-plugin": "0.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack-cli": "^3.0.8",
    "webpack": "^4.15.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "electron-rebuild": "^1.8.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.8",
    "js-yaml": "^3.12.2",
    "through2": "^2.0.3",
    "translation.js": "^0.7.3"
  }
}
